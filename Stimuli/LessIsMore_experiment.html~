<script>
var currentTrial = 1;
function NextTrial() {
	var checkedValue = $("input[type=radio]:checked", "#page" + currentTrial).val();
	if (checkedValue) {
		$("#page" + currentTrial).hide();
		$("#page" + (currentTrial+1)).show();
		currentTrial++;
	} else {
		alert('Please answer the question before moving on!');
	}
}




function Continue() {
	$("#page" + currentTrial).hide();
	$("#page" + (currentTrial+1)).show();
	currentTrial++;
}

function Ineligible() {
	$("#page" + currentTrial).hide();
	$("#page1000").show();
	currentTrial++;
}

function Skip() {
	var checkedValue = $("input[type=radio]:checked", "#page" + currentTrial).val();
	if (checkedValue == "yes") {
		$("#page" + currentTrial).hide();
		$("#page" + (currentTrial+1)).show();
		currentTrial++; }
	else if (checkedValue == "no") {
		$("#page" + currentTrial).hide();
		$("#page" + (currentTrial+2)).show();
		currentTrial++;
	} else {
		alert('Please answer the question before moving on!');
	}
}

function IsConsent() {
	var checkedValue = $("input[type=radio]:checked", "#page" + currentTrial).val();
	if (checkedValue == "consent") {
		$("#page" + currentTrial).hide();
		$("#page" + (currentTrial+1)).show();
		currentTrial++; }
	else if (checkedValue == "notconsent") {
		alert('Please skip this HIT.');		
	} else {
		alert('Please answer the question before moving on!');
	}
}

function validateAge() {
	var checkedAge = $("#Age").val();
	if (checkedAge != "") {
		javascript:CheckAge(); }
	else {
		alert("Please Select Your Age");
   	}
}


function CheckAge() {
	var checkedAge = $("#Age").val();
	if(checkedAge > 17) { 
		javascript:Continue(); 
		} else{javascript:Ineligible(); }
	}


$("form").keypress(function(e) {
  //Enter key
  if (e.which == 13) {
    return false;
  }
});
function CheckSummary1() {
	var description = $.trim($('#trial1_summary').val());
	if(description.length == 0){
	    alert('Please write a short summary before continuing');}
	else{javascript:Continue();}
}



function CheckSummary2() {
	var description = $.trim($('#trial2_summary').val());
	if(description.length == 0){
	    alert('Please write a short summary before continuing');}
	else{javascript:Continue();}
}


