	else{javascript:Continue();}
}



function CheckSummary7() {
	var description = $.trim($('#trial7_summary').val());
	if(description.length == 0){
	    alert('Pease write a short summary before continuing');}
	else{javascript:Continue();}
}



function CheckSummary8() {
